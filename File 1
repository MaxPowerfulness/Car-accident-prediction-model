import pandas as pd
import geopandas as gpd
import matplotlib.pyplot as plt

def main():
    states = gpd.read_file('USMap.json')
    states = states[(states['NAME'] != 'Alaska') &
                    (states['NAME'] != 'Hawaii')]
    # print(states.head())
    states.plot()
    plt.savefig('US.png', bbox_inches='tight')
if __name__ == '__main__':
    main()
